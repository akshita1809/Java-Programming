class Solution {
    public int[] twoSum(int[] nums, int target) {
        int i=0;
        int j=0;
        int newarr[]=new int [2];
        int n= nums.length;
        for (i=0; i<n; i++){
            for (j=i+1; j<n; j++){
                if (nums[i]+nums[j]==target){
                    newarr[0]=j;
                    newarr[1]=i;
                    break;
                }
                
            }
            
        }
    return newarr;    
     
    }
}
